{
    "name": "azaharizaman/huggingface-php",
    "description": "A modern PHP client for the Hugging Face Inference API with full type safety and comprehensive model support",
    "keywords": ["huggingface", "ai", "ml", "nlp", "transformers", "inference", "api", "client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Azahari Zaman",
            "email": "azaharizaman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "php-http/discovery": "^1.15.3",
        "php-http/multipart-stream-builder": "^1.4",
        "psr/http-client": "^1.0.2",
        "psr/http-client-implementation": "^1.0.1",
        "psr/http-factory-implementation": "*",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5.1",
        "guzzlehttp/psr7": "^2.5.0",
        "phpunit/phpunit": "^11.0",
        "slevomat/coding-standard": "^8.8",
        "symfony/http-client": "^7.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "AzahariZaman\\Huggingface\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AzahariZaman\\Huggingface\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "format": "vendor/bin/phpcbf",
        "lint": "vendor/bin/phpcs",
        "analyse": "vendor/bin/phpstan analyse",
        "check": [
            "@lint",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
